<div><a href="{{ path('contact_add') }}">Ajouter un contact</a></div>
<h1>Liste des contacts</h1>
{% for contact in contacts %}
    <div>
        <span>{{ contact.firstName|e }}</span>
        <span>{{ contact.lastName|e }}</span>
        <span>{{ contact.email|e }}</span>
        <a href="{{ path('contact_edit', {'id': contact.id}) }}">Editer</a>
    </div>
{% endfor %}